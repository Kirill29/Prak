@{
    ViewBag.Title = "Главная страница";
}
<!-- <script src="/js/Ramka"></script>    -->
<head>
    <link rel="stylesheet" href="/css/leaflet.css" />
    <script src="/js/leaflet.js" type="text/javascript"></script>
    <script src="/js/shp.js" type="text/javascript"></script>
    <script src="/js/leaflet.shpfile.js" type="text/javascript"></script>
    <script src="/js/wicket.js" type="text/javascript"></script>
    <script src="/js/wicket-gmap3.js" type="text/javascript"></script>
    <script src="/js/leaflet-offline.js" type="text/javascript"></script>

</head>
<body>




    <div id="mapid" style="width: 800px; height: 500px;"></div>



    @using (Html.BeginForm())
    {
        <label>Имя рамки</label>
        <input type="text" name="cmr_name_value" id="cmr_name_value" />
        <label>WKT строка</label>
        <input type="text" name="WKT_string" id="Wkt_string" />
        <button asp-action="Add" asp-controller="File">Добавить</button>

    }

    <form action='#' onsubmit="return false;">
        <input type='file' id='fileinput'>
        <input type='button' id='btnLoad' value='Load' onclick='loadFile();'>
    </form>

    <button onclick='Display_shp()'>добавить шейп</button>

    <button onclick='Display()'>Отобразить рамку</button>

    <button asp-action="Add_shp" asp-controller="File">Вернем геоджосон</button>








    <script src="/js/Ramka1.js" type="text/javascript"></script>










</body>





